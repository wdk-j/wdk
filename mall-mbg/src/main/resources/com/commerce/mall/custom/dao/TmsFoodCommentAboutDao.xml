<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.commerce.mall.custom.dao.TmsFoodCommentAboutDao">
    <select id="countNiceEqualTo" resultType="java.lang.Integer">
        select count(*) sales from tms_food_comments where food_id= #{foodId} and nice= #{keyword}
    </select>
</mapper>